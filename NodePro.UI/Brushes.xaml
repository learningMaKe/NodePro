<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="PrimaryColor">#A3E635</Color>
    <Color x:Key="SecondaryColor">#FEFDFA</Color>
    <Color x:Key="BackgroundColor">#F0F4F1</Color>
    <Color x:Key="TextPrimaryColor">#B6BEB5</Color>
    <Color x:Key="TextActiveColor">#FFFFFF</Color>
    <Color x:Key="TextSecondaryColor">#FF757575</Color>

    <Color x:Key="NodePrimaryColor">#385F2D</Color>
    <Color x:Key="NodeBackgroundColor">#303030</Color>
    <Color x:Key="NodeConnectorPrimaryColor">#A1A1A1</Color>

    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}" />
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource SecondaryColor}" />
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource BackgroundColor}" />

    <SolidColorBrush x:Key="TextPrimaryBrush" Color="{StaticResource TextPrimaryColor}" />
    <SolidColorBrush x:Key="TextActiveBrush" Color="{StaticResource TextActiveColor}" />
    <SolidColorBrush x:Key="TextSecondaryBrush" Color="{StaticResource TextSecondaryColor}" />

    <SolidColorBrush x:Key="NodePrimaryBrush" Color="{StaticResource NodePrimaryColor}" />
    <SolidColorBrush x:Key="NodeBackgroundBrush" Color="{StaticResource NodeBackgroundColor}" />

    <SolidColorBrush x:Key="NodeConnectorPrimaryBrush" Color="{StaticResource NodeConnectorPrimaryColor}" />

</ResourceDictionary>